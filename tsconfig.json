{
  "compileOnSave": true,
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "build",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "typeRoots": [ "./typings", "./node_modules/@types"],
    "strictPropertyInitialization": false,
    "moduleResolution": "node"
  },
  "exclude": [
    "dist",
    "node_modules",
    "test"
  ]
}
